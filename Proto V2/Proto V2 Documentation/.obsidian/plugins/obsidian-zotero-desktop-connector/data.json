{
  "database": "Zotero",
  "noteImportFolder": "Research",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "png",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Format #1",
      "format": "formatted-citation",
      "cslStyle": "ieee-with-url"
    }
  ],
  "exportFormats": [
    {
      "name": "Import #1",
      "outputPathTemplate": "Research/{{citekey}}/{{citekey}}.md",
      "imageOutputPathTemplate": "Research/{{citekey}}/",
      "imageBaseNameTemplate": "image"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": false,
  "whichNotesToOpenAfterImport": "first-imported-note",
  "pdfExportImageOCR": true,
  "pdfExportImageTesseractPath": "/usr/bin/tesseract"
}